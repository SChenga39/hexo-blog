<%- css_ex(theme.static_prefix.bootstrap, 'css/bootstrap.min.css') %>

<% var css_snippets = deduplicate(page.css_snippets) %>
<% for (var idx = 0; idx < css_snippets.length; idx++) { %>
  <%- css_snippets[idx] %>
<% } %>
<% page.css_snippets = [] %>

